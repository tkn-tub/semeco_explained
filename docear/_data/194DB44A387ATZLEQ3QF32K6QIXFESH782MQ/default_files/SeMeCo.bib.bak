  keywords = {Amino acid analysis, Amino acid metabolism, Bacteria, Biochemical
	simulations, Cell metabolism, Metabolites, Population genetics, Species
	diversity},
  language = {en},
  url = {https://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1004986},
  urldate = {2025-03-04}
}

