  keywords = {Metabolic engineering, Metabolic network modeling, Microbial communities,
	Synthetic Biology, Systems Biology},
  series = {Engineering {Tools} and {Prospects} in {Synthetic} {Biology}},
  shorttitle = {Synthetic {Ecology} of {Microbes}},
  url = {https://www.sciencedirect.com/science/article/pii/S0022283615006129},
  urldate = {2025-03-04}
}

@ARTICLE{zomorrodi_mathematical_2012,
  author = {Zomorrodi, Ali R. and Suthers, Patrick F. and Ranganathan, Sridhar
