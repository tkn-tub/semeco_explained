  volume = {16},
  pages = {383--390},
  number = {6},
  month = {\#jun\#},
  note = {Publisher: Nature Publishing Group},
  abstract = {Microorganisms engage in complex interactions with other organisms
	and their environment. Recent studies have shown that these interactions
	are not limited to the exchange of electron donors. Most microorganisms
	are auxotrophs, thus relying on external nutrients for growth, including
	the exchange of amino acids and vitamins. Currently, we lack a deeper
	understanding of auxotrophies in microorganisms and how nutrient
	requirements differ between different strains and different environments.
	In this Opinion article, we describe how the study of auxotrophies
	and nutrient requirements among members of complex communities will
	enable new insights into community composition and assembly. Understanding
	this complex network over space and time is crucial for developing
	strategies to interrogate and shape microbial communities.},
  copyright = {2018 Macmillan Publishers Ltd., part of Springer Nature},
  doi = {10.1038/s41579-018-0004-5},
  file = {Zengler and Zaramela - 2018 - The social network of microorganisms — how auxotro.pdf:C\:\\Users\\jorge\\Zotero\\storage\\UAH34HVA\\Zengler and Zaramela - 2018 - The social network of microorganisms — how auxotro.pdf:application/pdf},
  issn = {1740-1534},
  keywords = {Bacterial infection, Microbial ecology, Microbiome, Microbiota},
  language = {en},
  url = {https://www.nature.com/articles/s41579-018-0004-5},
  urldate = {2025-02-18}
}

@ARTICLE{zhuang2011genome,
